{
  "env": {
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": [".eslintrc.js"],
  "rules": {
    "no-underscore-dangle": ["error", {
      "allow": ["_id"]
    }],
    "linebreak-style": 0,
    "no-unused-vars": "off",
    "consistent-return": 0
  }
}